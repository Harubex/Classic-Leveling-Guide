{
    "restartable": "rs",
    "ignore": [
        ".git",
        "node_modules/",
        "dist/",
        "coverage/"
    ],
    "watch": [
        "src/",
        "../shared/steps.json"
    ],
    "execMap": {
        "ts": "node -r dotenv/config -r ts-node/register"
    },
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts,json"
}